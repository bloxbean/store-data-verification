{
  "compilerOptions": {
    "outDir": "./build",
    "baseUrl": ".",
    "module": "esnext",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "rootDir": "./",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": true,
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": true,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@api-common/*": ["TS-playwright-api-testing/common/*"],
      "@api-tests/*": ["TS-playwright-api-testing/tests/*"],
      "@api-helpers/*": ["TS-playwright-api-testing/common/helpers/*"],
      "@api-utils/*": ["TS-playwright-api-testing/common/utils/*"],
      "@api-fixture/*": ["TS-playwright-api-testing/tests/fixture/*"],
      "@api-hooks/*": ["TS-playwright-api-testing/tests/hooks/*"],
      "@api-env/*": ["TS-playwright-api-testing/env/*"],
      "@integration-common/*": ["TS-playwright-integration-testing/common/*"],
      "@integration-tests/*": ["TS-playwright-integration-testing/tests/*"],
      "@integration-helpers/*": [
        "TS-playwright-integration-testing/common/helpers/*"
      ],
      "@integration-utils/*": [
        "TS-playwright-integration-testing/common/utils/*"
      ],
      "@integration-fixture/*": [
        "TS-playwright-integration-testing/tests/fixture/*"
      ],
      "@integration-hooks/*": [
        "TS-playwright-integration-testing/tests/hooks/*"
      ],
      "@integration-env/*": ["TS-playwright-integration-testing/env/*"]
    },
    "typeRoots": ["./node_modules/@types"]
  }
}
